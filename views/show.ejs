<div>
    <% events.forEach(function(event){ %>
        <p>Name: <%= event.name %></p>
        <p>Description: <%= event.description %></p>
        <a href="/show/<%= event._id%>">show this event</a>
        <a href="/events/<%= event._id%>">Edit this event</a>
        <br>
        <a href="/events/delete/<%= event._id%>">Remove this event</a>
        <br>
    <% }) %>
    <a href="/"></a>
</div>